{
  "name": "@herokuro/website",
  "version": "0.0.0-development",
  "description": "A set of Node.js tools and utilities to help kickstart and manage your Heroku projects.",
  "private": true,
  "author": "Richard King <richrdkng@gmail.com> (www.richrdkng.com)",
  "license": "ISC",
  "homepage": "https://github.com/herokuro/website#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herokuro/website.git"
  },
  "bugs": {
    "url": "https://github.com/herokuro/website/issues"
  },
  "keywords": [
    "herokuro",
    "@herokuro",
    "heroku",
    "website",
    "tool",
    "tools",
    "util",
    "utils",
    "utility",
    "utilities"
  ],
  "engines": {
    "node": ">=10.16.13"
  },
  "standard": {
    "globals": [
      "$",
      "XMLHttpRequest"
    ]
  },
  "scripts": {
    "gulp": "gulp --gulpfile script/gulp/config.js",
    "dev": "rm -rf dev && webpack-dev-server --config script/webpack/config.js",
    "import:icons": "npm run --silent gulp -- import:icons",
    "import:meta": "npm run --silent gulp -- import:meta",
    "build:app": "rm -rf build && webpack -p --config script/webpack/config.js",
    "build:docs": "npm run --silent gulp -- build:docs",
    "build": "npm run build:app && npm run build:docs",
    "check:build": "npm run build",
    "check:style": "standard --verbose | snazzy",
    "deploy": "npm run --silent gulp -- deploy",
    "test": ""
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@herokuro/brand": "0.0.1",
    "@herokuro/config-vars": "0.0.1",
    "@herokuro/maintenance": "0.0.1",
    "@herokuro/noop-server": "^1.0.3",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/exec": "^3.3.6",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.3",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.2.1",
    "css-loader": "^3.2.0",
    "execa": "^2.0.4",
    "fibers": "^4.0.1",
    "file-loader": "^4.2.0",
    "get-value": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-remote-src": "^0.4.4",
    "gulp-rename": "^1.4.0",
    "gulp-tap": "^2.0.0",
    "handlebars": "^4.2.0",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "html5-boilerplate": "^7.2.0",
    "is-ci": "^2.0.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "parse-author": "^2.0.0",
    "pkg-dir": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "semantic-release": "^15.13.24",
    "semver": "^6.3.0",
    "snazzy": "^8.0.0",
    "standard": "^14.2.0",
    "style-loader": "^1.0.0",
    "svg-sprite-loader": "^4.1.6",
    "webpack": "^4.40.1",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
